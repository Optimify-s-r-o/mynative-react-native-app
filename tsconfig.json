    "strict": true,
    "baseUrl": "./",
    "paths": {
      "assets/*": ["./src/assets/*"],
      "constants/*": ["./src/constants/*"],
      "context/*": ["./src/context/*"],
      "hooks/*": ["./src/hooks/*"],
      "navigation/*": ["./src/navigation/*"],
      "screens/*": ["./src/screens/*"],
      "styles/*": ["./src/styles/*"],
